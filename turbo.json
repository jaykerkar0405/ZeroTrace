{
  "ui": "tui",
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "type-check": {
      "dependsOn": ["^type-check"],
      "outputs": ["*.tsbuildinfo"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**", "artifacts/**"],
      "env": ["PINATA_JWT", "NEXT_PUBLIC_PINATA_GATEWAY", "NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID"]
    }
  }
}
