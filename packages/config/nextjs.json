{
  "display": "Next.js",
  "extends": "./base.json",
  "exclude": ["node_modules"],
  "$schema": "https://json.schemastore.org/tsconfig",
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "noEmit": true,
    "allowJs": true,
    "jsx": "preserve",
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "lib": ["DOM", "DOM.Iterable", "ES2022"]
  }
}
